---
---
RewriteEngine On
RewriteBase /

{% for post in site.posts %}
{% if post.original != null %}
RewriteRule ^{{ post.original }} {{ post.url }} [R=301,L]
{% endif %}
{% endfor %}

{% for page in site.pages %}
{% if page.original != null %}
RewriteRule ^{{ page.original }} {{ page.url }} [R=301,L]
{% endif %}
{% endfor %}
